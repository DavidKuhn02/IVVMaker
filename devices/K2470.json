{
  "name": "K2400",
  "type": "smu",
  "init_commands": [
    ":SOUR:FUNC VOLT",
    ":SOUR:VOLT 0"
  ],
  "commands": {
    "reset": "*RST",
    "clear_buffer": [
      "*CLS",
      "TRAC:CLE \"defbuffer1\"",
      "TRAC:CLE \"defbuffer2\""
    ],
    "return_id": "*IDN?",
    "enable_highC": ":SOUR:VOLT:HIGH:CAP ON",
    "disable_highC": ":SOUR:VOLT:HIGH:CAP OFF",
    "set_voltage_range": ":SOUR:VOLT:RANG {range}",
    "set_voltage_range_auto_on": ":SOUR:VOLT:RANG:AUTO ON",
    "set_voltage_range_auto_off": ":SOUR:VOLT:RANG:AUTO OFF",
    "set_current_range": ":SENS:CURR:RANG {range}",
    "set_current_range_auto_on": ":SENS:CURR:RANG:AUTO ON",
    "set_current_range_auto_off": "SENS:CURR:RANG:AUTO OFF",
    "set_filter_num": ":SENS:CURR:AVER:COUN {filter_num};",
    "set_filter_type": ":SENS:CURR:AVER:TCON {filter_type};",
    "enable_filter": ":SENS:CURR:AVER ON",
    "disable_filter": ":SENS:CURR:AVER OFF",
    "set_nplc": ":SENS:CURR:NPLC {nplc}",
    "enable_auto_zero": ":SENS:CURR:AZER ON",
    "disable_auto_zero": ":SENS:CURR:AZER OFF",
    "enable_output": ":OUTP {state}",
    "set_limit": ":SOUR:VOLT:ILIM {limitI}",
    "set_voltage": ":SOUR:VOLT {voltage}",
    "measure_current": ":MEAS:CURR?",
    "measure_voltage": ":MEAS:VOLT?",
    "measure_resistance": ":MEAS:RES?"
  },
  "ui_settings": [
    {
      "name": "limitI",
      "label": "Current Limit [uA]",
      "type": "float",
      "default": 10,
      "min": 0.0,
      "max": 1000000.0,
      "step": 0.1,
      "function": "set_limit",
      "values": [
        "limitI"
      ]
    },
    {
      "name": "voltage_range",
      "label": "Voltage Range",
      "type": "select",
      "options": [
        "Auto",
        "200mV",
        "2V",
        "20V",
        "200V",
        "1000V"
      ],
      "default": "Auto",
      "function": "set_voltage_range",
      "values": [
        "voltage_range"
      ]
    },
    {
      "name": "current_range",
      "label": "Current Range",
      "type": "select",
      "options": [
        "Auto",
        "10nA",
        "100nA",
        "1uA",
        "10uA",
        "100uA",
        "1mA",
        "10mA",
        "100mA",
        "1A"
      ],
      "default": "Auto",
      "function": "set_current_range",
      "values": [
        "current_range"
      ]
    },
    {
      "name": "nplc",
      "label": "NPLC",
      "type": "int",
      "default": 1,
      "min": 0,
      "max": 10,
      "step": 1,
      "function": "set_nplc",
      "values": [
        "nplc"
      ]
    },
    {
      "name": "high_capacitance",
      "label": "High Capacitance",
      "type": "checkbox",
      "default": false,
      "function": "set_highC",
      "values": [
        "high_capacitance"
      ]
    },
    {
      "name": "use_filter",
      "label": "Use Filter",
      "type": "checkbox",
      "default": false,
      "function": "set_filter",
      "values": [
        "use_filter",
        "filter_num",
        "filter_type"
      ]
    },
    {
      "name": "filter_num",
      "label": "Filter Number",
      "type": "int",
      "default": 10,
      "min": 1,
      "max": 100,
      "step": 1,
      "function": "set_filter",
      "values": [
        "use_filter",
        "filter_num",
        "filter_type"
      ]
    },
    {
      "name": "filter_type",
      "label": "Filter Type",
      "type": "select",
      "options": [
        "Moving Average",
        "Repeat Average"
      ],
      "default": "Moving Average",
      "function": "set_filter",
      "values": [
        "use_filter",
        "filter_num",
        "filter_type"
      ]
    },
    {
      "name": "auto_zero",
      "label": "Auto Zero",
      "type": "checkbox",
      "default": true,
      "function": "set_auto_zero",
      "values": [
        "auto_zero"
      ]
    }
  ]
}