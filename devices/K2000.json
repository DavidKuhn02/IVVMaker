{
  "name": "K2000",
  "type": "multimeter",
  "init_commands": [],
  "settings": {
    "measurement_type": "VOLD:DC",
    "voltage_range": "Auto",
    "current_range": "Auto",
    "resistance_range": "Auto",
    "use_filter": false,
    "filter_num": 10,
    "filter_type": "Moving Average"
  },
  "commands": {
    "reset": "*RST",
    "clear_buffer": "*CLS",
    "return_id": "*IDN?",
  "set_measurement_type": "SENS:FUNC \"{quantity}\"",
  "set_voltage_range": ":SENS:VOLT:RANG {range}",
  "set_voltage_range_auto_on": ":SENS:VOLT:RANG:AUTO ON",
  "set_voltage_range_auto_off": ":SENS:VOLT:RANG:AUTO OFF",
  "set_current_range": ":SENS:CURR:RANG {range}",
  "set_current_range_auto_on": ":SENS:CURR:RANG:AUTO ON",
  "set_current_range_auto_off": ":SENS:CURR:RANG:AUTO OFF",
  "set_resistance_range": ":SENS:RES:RANG {range}",
  "set_resistance_range_auto_on": ":SENS:RES:RANG:AUTO ON",
  "set_resistance_range_auto_off": ":SENS:RES:RANG:AUTO OFF",
  "set_filter_num": ":SENS:CURR:AVER:COUN {filter_num};",
  "set_filter_type": ":SENS:CURR:AVER:TCON {filter_type};",
  "enable_filter": ":SENS:CURR:AVER ON",
  "disable_filter": ":SENS:CURR:AVER OFF",
  "measure": "READ?"
  },
  "ui_settings": [
    {"name": "measurement_type", "type": "select", "label": "Measurement Type", "options": ["Voltage DC", "Current DC", "Resistance (2 Wire)", "Voltage AC", "Current AC", "Resistance (4 Wire)", "Frequency", "Period"], "default": "Voltage DC", "function": "set_measurement_type", "values" : "measurement_type"},
    {"name": "voltage_range", "type": "select", "label": "Voltage Range", "options": ["Auto", "100mV", "1V", "10V", "100V", "1000V"], "default": "Auto", "function": "set_voltage_range", "values": "voltage_range"},
    {"name": "current_range", "type": "select", "label": "Current Range", "options": ["Auto", "1mA", "10mA", "100mA", "1A"], "default": "Auto", "function": "set_current_range", "values": "current_range"},
    {"name": "resistance_range", "type": "select", "label": "Resistance Range", "options": ["Auto", "100 Ohm", "1k Ohm", "10k Ohm", "100k Ohm", "1M Ohm", "10M Ohm"], "default": "Auto", "function": "set_resistance_range", "values": "resistance_range"},
    {"name": "use_filter", "type": "checkbox", "label": "Use Filter", "default": false, "function": "set_filter", "values": ["use_filter", "filter_num", "filter_type"]},
    {"name": "filter_num", "type": "int", "label": "Filter Count", "min": 1, "max": 100, "step": 1, "default": 10, "function": "set_filter", "values": ["use_filter", "filter_num", "filter_type"]},
    {"name": "filter_type", "type": "select", "label": "Filter Type", "options": ["Moving Average", "Repeat Average"], "default": "Moving Average", "function": "set_filter", "values": ["use_filter", "filter_num", "filter_type"]}
  ]
}
